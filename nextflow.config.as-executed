         args = '--id_col 1 --sample_suffix \'\' --outprefix deseq2 --count_col 3'
         RData = ''
         pca {
            vals {
               txt = ''
            }
         }
         plots {
            pdf = ''
         }
         sample {
            dists {
               txt = ''
            }
         }
         size_factors = ''
         log = ''
         publish_files = ['', '', '', '', '', '']
         publish_dir = 'star_salmon/deseq2_qc'
      }
      qualimap_rnaseq {
         publish_dir = 'star_salmon/qualimap'
      }
      dupradar {
         Dens {
            pdf = 'scatter_plot'
         }
         Boxplot {
            pdf = 'box_plot'
         }
         Hist {
            pdf = 'histogram'
         }
         Matrix {
            txt = 'gene_data'
         }
         slope {
            txt = 'intercepts_slope'
         }
         publish_files = ['scatter_plot', 'box_plot', 'histogram', 'gene_data', 'intercepts_slope']
