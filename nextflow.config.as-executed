         pdf = 'pdf'
         r = 'rscript'
         xls = 'xls'
         publish_files = ['pdf', 'rscript', 'xls']
         publish_dir = 'star_salmon/rseqc/read_duplication'
      }
      multiqc {
         args = ''
         publish_dir = 'multiqc/star_salmon'
