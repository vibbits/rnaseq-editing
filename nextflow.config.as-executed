process {
   cpus = 1
   memory = '6 GB'
   time = '4h'
   errorStrategy = { task.exitStatus in [143,137,104,134,139] ? 'retry' : 'finish' }
   maxRetries = 1
   maxErrors = '-1'
   withLabel:process_low {
      cpus = 2
      memory = '12 GB'
      time = '4h'
   }
   withLabel:process_medium {
      cpus = 6
      memory = '45 GB'
      time = '10d 10h'
   }
   withLabel:process_high {
      cpus = 6
      memory = '55 GB'
      time = '16h'
   }
   withLabel:process_long {
      cpus = 12
      time = '10d 10h'
   }
   withLabel:process_high_memory {
      memory = '200 GB'
   }
   withLabel:error_ignore {
      errorStrategy = 'ignore'
   }
   withLabel:error_retry {
      errorStrategy = 'retry'
      maxRetries = 2
   }
   executor = 'k8s'
   scratch = true
}

env {
   PYTHONNOUSERSITE = 1
   R_PROFILE_USER = '/.Rprofile'
   R_ENVIRON_USER = '/.Renviron'
}

timeline {
   enabled = true
   file = './results/pipeline_info/execution_timeline_2021-10-01_08-54-16.html'
}

report {
   enabled = true
   file = './results/pipeline_info/execution_report_2021-10-01_08-54-16.html'
}

trace {
   enabled = true
   file = './results/pipeline_info/execution_trace_2021-10-01_08-54-16.txt'
}

dag {
   enabled = true
   file = './results/pipeline_info/pipeline_dag_2021-10-01-30_08-54-16.svg'
}

manifest {
   name = 'nf-core/rnaseq'
   author = 'Phil Ewels, Rickard Hammarén'
   homePage = 'https://github.com/nf-core/rnaseq'
   description = 'Nextflow RNA-Seq analysis pipeline, part of the nf-core community.'
   mainScript = 'main.nf'
   nextflowVersion = '!>=21.04.0'
   version = '3.3'
}

k8s {
   debug {
      yaml = true
   }
   storageClaimName = 'azurefile-nextflow-work'
   storageMountPath = '/work'
   namespace = 'nextflow'
   projectDir = '/work/projects'
   launchDir = '/work/launch'
   workDir = '/work/nf-work'
   pod = [[volumeClaim:'azurefile-nextflow-input', mountPath:'/input'], [nodeSelector:'agentpool=sys'], [volumeClaim:'azurefile-nextflow-output', mountPath:'/output']]
}

docker {
   enabled = true
   userEmulation = true
}

singularity {
   enabled = false
}

podman {
   enabled = false
}

shifter {
   enabled = false
}

charliecloud {
   enabled = false
}

runName = 'irreverent-curran'
resume = null
