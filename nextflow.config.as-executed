            tsv = 'runinfo'
         }
      }
      sra_runinfo_to_ftp {
         publish_dir = 'public_data'
         publish_files {
            tsv = 'runinfo'
         }
      }
      sra_fastq_ftp {
         publish_dir = 'public_data'
         fastq {
            gz = ''
         }
         md5 = 'md5'
