         publish_dir = 'star_salmon/rseqc/inner_distance'
      }
      rseqc_inferexperiment {
         publish_dir = 'star_salmon/rseqc/infer_experiment'
      }
      rseqc_junctionannotation {
         pdf = 'pdf'
         r = 'rscript'
         bed = 'bed'
         xls = 'xls'
         log = 'log'
         publish_files = ['pdf', 'rscript', 'bed', 'xls', 'log']
