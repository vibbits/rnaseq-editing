         pdf = 'pdf'
         r = 'rscript'
         publish_files = ['pdf', 'rscript']
         publish_dir = 'star_salmon/rseqc/junction_saturation'
      }
      rseqc_readdistribution {
         publish_dir = 'star_salmon/rseqc/read_distribution'
      }
